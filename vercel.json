{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "./public/**",
      "use": "@now/static"
    }
  ],
  "headers": [
    {
      "source": "/music/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=2592000"
        }
      ]
    },
    {
      "source": "/live/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=2592000"
        }
      ]
    }
  ]
}
